    "autoload": {
        "psr-4": {
            "Mahmud2074\\QueryLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mahmud2074\\QueryLogger\\QueryLoggerServiceProvider"
